<h3>List of users</h3>

<%= paginate @users %>

<table border="1">
  <thead>
    <th>Name:</th>
    <th>Email:</th>
    <th>Admin Status:</th>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.full_name %></td>
        <td><%= user.email %></td>
        <td><%= user.admin %> <%=admin_user_path(user)%></td>
        <td><%= button_to "Delete User", admin_user_path(user), method: :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>